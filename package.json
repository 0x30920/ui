        "vuex": "*"
    },
    "devDependencies": {
        "@vue/cli": "*",
        "@vue/cli-plugin-babel": "*",
        "@vue/cli-plugin-eslint": "*",
        "@vue/cli-service": "*",
        "@vue/eslint-config-prettier": "*",
        "eslint": "^5.14.1",
        "html-webpack-plugin": "*",
        "html-webpack-template": "*",
        "lint-staged": "^8.1.4",
