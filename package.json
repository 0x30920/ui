{
  "name": "epicsevendb-ui",
  "version": "0.8.0",
  "private": true,
  "license": "CC-BY-NC-SA-4.0",
  "data": {
    "apiEndpoint": "https://epicsevendb-apiserver.herokuapp.com/api/",
    "assetsEndpoint": "https://assets.epicsevendb.com",
    "analyticsId": "UA-129320031-1"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "rm -rf ./dist; vue-cli-service build --modern && zip -r -9 dist.zip ./dist",
    "dist": "serve -d dist",
    "lint": "vue-cli-service lint --fix",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "axios": "*",
    "core-js": "^2.6.5",
    "modernizr": "*",
    "normalize.css": "*",
    "nprogress": "*",
    "v-tooltip": "^2.0.1",
    "vue": "^2.6.6",
    "vue-analytics": "*",
    "vue-axios": "*",
    "vue-lazyload": "^1.2.6",
    "vue-multiselect": "^2.1.3",
    "vue-router": "^3.0.6",
    "vue-tabs-component": "*",
    "vuex": "*",
    "snyk": "^1.161.1"
  },
  "devDependencies": {
    "@vue/cli": "*",
    "@vue/cli-plugin-babel": "*",
    "@vue/cli-plugin-eslint": "*",
    "@vue/cli-service": "*",
    "@vue/eslint-config-prettier": "*",
    "eslint": "^5.14.1",
    "html-webpack-plugin": "*",
    "html-webpack-template": "*",
    "lint-staged": "^8.1.4",
    "node-sass": "*",
    "sass-loader": "*",
    "serve": "*",
    "vue-template-compiler": "^2.6.6",
    "webpack-modernizr-loader": "*"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "snyk": true
}
