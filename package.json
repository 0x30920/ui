{
  "name": "epicsevendb-ui",
  "version": "1.4.0",
  "description": "The Epic Seven Wiki and Database with information on Heroes, Artifacts, Catalysis, Items, Quests, Guides and more!",
  "author": "RaphaelDDL <raphaelddl@epicsevendb.com>",
  "repository": "https://github.com/EpicSevenDB/ui",
  "homepage": "https://epicsevendb.com",
  "bugs": {
    "url": "https://github.com/EpicSevenDB/ui/issues"
  },
  "private": true,
  "license": "CC-BY-NC-SA-4.0",
  "data": {
    "apiEndpoint": "https://epicsevendb-apiserver.herokuapp.com/api/",
    "assetsEndpoint": "https://assets.epicsevendb.com",
    "analyticsId": "UA-129320031-1"
  },
  "engines": {
    "node": ">=10"
  },
  "lint-staged": {
    "linters": {
      "*.{js,vue}": [
        "eslint --fix",
        "git add"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.5.3",
    "@nuxtjs/google-analytics": "^2.2.0",
    "@nuxtjs/pwa": "^2.6.0",
    "connect-timeout": "^1.9.0",
    "cross-env": "^5.2.0",
    "dateformat": "^3.0.3",
    "express": "^4.17.1",
    "express-rate-limit": "^4.0.3",
    "helmet": "^3.18.0",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.8.1",
    "nuxt-i18n": "^5.12.4",
    "v-tooltip": "^2.0.2",
    "vue-aplayer": "^1.6.1",
    "vue-i18n": "^8.11.2",
    "vue-js-modal": "^1.3.31",
    "vue-lazyload": "^1.2.6",
    "vue-multiselect": "^2.1.6",
    "vue-tabs-component": "^1.5.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "husky": "^2.3.0",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.7",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.16.4",
    "sass-loader": "^7.1.0",
    "vue-jest": "^3.0.3"
  },
  "heroku-run-build-script": true
}
