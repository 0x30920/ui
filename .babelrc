{
	"env": {
		"production": {
			"presets": ["minify"]
		}
	},
	"presets": [
		[
			"@babel/env",
			{
				"targets": {
					"node": "current"
				}
			}
		]
	],
	"plugins": [
		[
			"@babel/transform-runtime",
			{
				"regenerator": true
			}
		],
		"@babel/plugin-proposal-nullish-coalescing-operator",
		"@babel/plugin-proposal-optional-chaining"
	]
}
